<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" namespace="Iridescent.Entities" assembly="Iridescent.Entities">
	<class name="Category,Iridescent.Entities" table="Category" lazy="true">

		<!-- Primary Key(s) -->
		<id name="Id" column="Id" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		
		<!-- Properties -->
		<property column="Name" type="String" name="Name" not-null="true" length="50" />
		<property column="DiaryCount" type="Int32" name="DiaryCount" not-null="true" />
		<property column="CreateDate" type="DateTime" name="CreateDate" not-null="true" />
		<property column="IsEnabled" type="Boolean" name="IsEnabled" not-null="true" />
		
	</class>
</hibernate-mapping>
